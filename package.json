{
  "name": "cclean-killer",
  "version": "1.0.0",
  "description": "The CCleaner Killer - System cleanup tool that outperforms the parasitic 'cleaner'",
  "scripts": {
    "test": "./tests/framework/test-runner.sh",
    "test:verbose": "./tests/framework/test-runner.sh --verbose",
    "test:coverage": "./tests/framework/test-runner.sh --coverage",
    "test:unit": "./tests/framework/test-runner.sh --filter unit",
    "test:integration": "./tests/framework/test-runner.sh --filter integration",
    "scan": "bash scripts/$(uname | tr '[:upper:]' '[:lower:]' | sed 's/darwin/macos/')/scan.sh",
    "clean": "bash scripts/$(uname | tr '[:upper:]' '[:lower:]' | sed 's/darwin/macos/')/clean.sh --all",
    "clean:dry-run": "bash scripts/$(uname | tr '[:upper:]' '[:lower:]' | sed 's/darwin/macos/')/clean.sh --dry-run --all",
    "parasites": "bash scripts/$(uname | tr '[:upper:]' '[:lower:]' | sed 's/darwin/macos/')/find-parasites.sh",
    "orphans": "bash scripts/$(uname | tr '[:upper:]' '[:lower:]' | sed 's/darwin/macos/')/find-orphans.sh",
    "report": "node scripts/report.mjs"
  },
  "bin": {
    "cclean-killer": "./bin/cclean-killer.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brainbloodbarrier/CClean-Killer.git"
  },
  "keywords": [
    "cleanup",
    "macos",
    "linux",
    "system",
    "cache",
    "maintenance",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brainbloodbarrier/CClean-Killer/issues"
  },
  "homepage": "https://github.com/brainbloodbarrier/CClean-Killer#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "devDependencies": {},
  "dependencies": {}
}
